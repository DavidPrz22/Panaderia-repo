+        ${type === "submit" ? "bg-blue-500 text-white hover:bg-blue-600 border-none rounded-md px-4 py-2" : ""} 
+        ${type === "cancel" ? "text-black hover:bg-gray-100 border border-gray-300 rounded-md px-4 py-2" : ""}
